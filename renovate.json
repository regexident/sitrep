{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "allowedVersions": "!/^=/",
      "groupName": "dependencies",
      "groupSlug": "dependencies"
    }
  ]
}